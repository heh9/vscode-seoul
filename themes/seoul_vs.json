{
  "$schema": "vscode://schemas/color-theme",
  "name": "seoul_vscode",
  "include": "./seoul_defaults.json",
  "tokenColors": [
    {
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#000080"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#688763",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#D0B06A",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#F8D892"
      }
    },
    {
      "scope": "constant.regexp",
      "settings": {
        "foreground": "#8FAEAE"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#8FAE8A"
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#8FAE8A"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#8FAE8A"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.tag.jade",
        "entity.other.attribute-name.tag.pug"
      ],
      "settings": {
        "foreground": "#D0B16B",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.attribute.scss",
        "entity.other.attribute-name.scss"
      ],
      "settings": {
        "foreground": "#D1D1AD"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "beginning.punctuation.definition.quote.markdown",
      "settings": {
        "foreground": "#608b4e"
      }
    },
    {
      "scope": "beginning.punctuation.definition.list.markdown",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "meta.selector",
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "name": "brackets of XML/HTML tags",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": "meta.structure.dictionary.key.python",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#569cd6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#F3B2B0"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#8FAEAE"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#8FAEAE"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#8FAEAE"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#8FAEAE"
      }
    },
    {
      "name": "JavaScript string interpolation ${}",
      "scope": [
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.end.ts",
        "punctuation.definition.template-expression.end.js"
      ],
      "settings": {
        "foreground": "#8FAE8A"
      }
    },
    {
      "scope": [
        "support.type.vendored.property-name",
        "support.type.property-name",
        "variable.css",
        "variable.scss",
        "variable.other.less"
      ],
      "settings": {
        "foreground": "#D0B16B"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#F3B38E"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#ff0000",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#D8D791"
      }
    },
    {
      "scope": "keyword.operator.expression.delete",
      "settings": {
        "foreground": "#569cd6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof"
      ],
      "settings": {
        "foreground": "#D8D791",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin.metatag.php",
        "punctuation.section.embedded.end.metatag.php"
      ],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#F3B2B0",
        "fontStyle": "italic"
      }
    }
  ]
}
